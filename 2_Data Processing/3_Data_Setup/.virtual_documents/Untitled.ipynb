import json


#  Function to load a data from a file
def load_data(filename):
    with open(filename, "r") as f:
        data = json.load(f)
    return data


# Printing file data
data = load_data("Dummy_Store_Data.json")
print(data)





# Function to clean & Structure the data
def clean_data(data):
    for user in data:
        print(user)


clean_data(data)



